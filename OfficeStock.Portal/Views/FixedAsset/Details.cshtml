@model OfficeStock.Entity.FixedAsset

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Details</h2>
<div>
    <h4>FixedAsset</h4>
    <hr />
    <div class="col-md-6">
        <dl class="dl-horizontal">
            <dt>
                Grup Id
            </dt>
            <dd>
                @Html.DisplayFor(model => model.GroupId)
            </dd>
            <dt>
                Demirbaş No
            </dt>
            <dd>
                @Html.DisplayFor(model => model.OfficeStockNo)
            </dd>
            <dt>
                Tanım
            </dt>
            <dd>
                @Html.DisplayFor(model => model.Description)
            </dd>
            <dt>
                Kullanıma Başlangıç Tarihi
            </dt>
            <dd>
                @Html.DisplayFor(model => model.UsageStartDate)
            </dd>
            <dt>
                T Tarihi
            </dt>
            <dd>
                @Html.DisplayFor(model => model.ProcurementDate)
            </dd>
            <dt>
                Değer
            </dt>
            <dd>
                @Html.DisplayFor(model => model.Value)
            </dd>
